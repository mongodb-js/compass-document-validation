{
  "name": "@mongodb-js/compass-document-validation",
  "productName": "Compass Document Validation",
  "version": "8.2.0",
  "apiVersion": "2.0.0",
  "description": "Compass Document Validation Plugin",
  "main": "index.js",
  "scripts": {
    "ci": "npm run check && npm test",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' index.js",
    "compile": "npm run jsx && npm run less",
    "jsx": "babel ./src --out-dir ./lib --presets react",
    "preless": "lessc ./styles/compass/index.less ./lib/styles/compass/index.css",
    "less": "lessc ./styles/index.less ./lib/styles/index.css",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "prestart": "npm run compile",
    "start": "electron ./electron",
    "storybook": "start-storybook -p 9001",
    "test": "hadron-build test",
    "test-enzyme": "npm test -- --enzyme",
    "test-main": "npm test -- --main",
    "test-renderer": "npm test -- --renderer",
    "test-unit": "npm test -- --unit",
    "test-functional": "npm test -- --functional"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-select-plus": "^1.2.0"
  },
  "dependency-check": {
    "ignore": [
      "react-dom"
    ]
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.3.0",
    "babel-cli": "^6.10.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.0",
    "chai-enzyme": "1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "debug": "^2.2.0",
    "electron": "^2.0.7",
    "electron-mocha": "^5.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-mongodb-js": "^2.1.0",
    "font-awesome": "^4.7.0",
    "hadron-app": "^3.0.1",
    "hadron-app-registry": "^7.0.0",
    "hadron-build": "^19.0.2",
    "hadron-react-components": "^3.0.0",
    "hadron-reflux-store": "0.0.3",
    "hadron-spectron": "^1.0.0",
    "hadron-type-checker": "^2.0.0",
    "highlight.js": "^10.4.1",
    "jsdom": "^9.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^3.10.1",
    "mocha": "^4.1.0",
    "mongodb-data-service": "^12.0.0",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "mongodb-ns": "^2.0.0",
    "pre-commit": "^1.1.3",
    "prop-types": "^15.5.8",
    "react": "^16.6.0",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^16.6.0",
    "react-fontawesome": "^1.6.1",
    "react-select-plus": "^1.2.0",
    "reflux": "0.4.1",
    "reflux-state-mixin": "^0.7.0",
    "semver": "^5.3.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "watch": "^1.0.2"
  }
}
